    "@sveltejs/kit": "^2.25.1",
    "@sveltejs/vite-plugin-svelte": "^6.1.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.11.0",
    "globals": "^16.0.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "svelte": "^5.36.8",
    "svelte-check": "^4.3.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.37.0",
    "vite": "^7.0.5"
  }
