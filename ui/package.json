    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.12.4",
    "globals": "^16.4.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "svelte": "^5.39.6",
    "svelte-check": "^4.3.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1",
